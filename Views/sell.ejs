
<%- include('../partials/header') %>
<body id="body">
<section class="page-header">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="content">
                    <h1 class="page-name">Create a Listing</h1>
                    <ol class="breadcrumb">
                        <li><a href="index.html">Home</a></li>
                        <li class="active">Buy</li>
                    </ol>
                </div>
            </div>
        </div>
    </div>
</section>
<section class="page-wrapper">
    <div class="contact-section">
        <div class="container">
            <div class="row">
                <!-- Contact Form -->
                <div class="contact-form col-md-6 ">
                    <form id="contact-form" method="post" action="" role="form">
                    
                        <div class="form-group">
                            <input type="text" placeholder="Listing Name" class="form-control" name="name" id="name">
                        </div>
                        
                        <div class="form-group">
                            <textarea rows="6" placeholder="Description" class="form-control" name="description" id="description"></textarea>   
                        </div>
                        
                        <div class="form-group">
                            <input type="text" placeholder="Asking Price" class="form-control" name="price" id="price">
                        </div>
                        
                        <div class="col-sm-8 mx-auto">
                            <div class="form-group">
                                <label for="sel1">Condition and category:</label>
                                <select class="form-control" id="sel1">
                                    <option>(Select condition)</option>
                                    <option>New</option>
                                    <option>Like new</option>
                                    <option>Used</option>
                                    <option>Worn</option>
            
                                </select>
                            </div>
                        <div>
                        
                        <div id="cf-submit">
                            <input type="submit" id="contact-submit" class="btn btn-transparent" value="Submit">
                        </div>                      
                        
                    </form>
                </div>
                <!-- ./End Contact Form -->
                
